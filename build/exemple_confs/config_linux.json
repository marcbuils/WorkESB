		"sequencers/nxt_driver.so":	{
			"enable":				false,
			"name":					"DRIVER NXT",
			"type":					"dll",					// required
			"variable_add":			"NXT_ADD",				// default: "NXT_ADD"
			"list":	[										// required
				{
					"type":				"sensor",
					"port":				"IN_1",
					"variable":			"NXT_PRESS"
				},
				{
					"type":				"motor",
					"port":				"OUT_A",
					"variable_speed": 	"NXT_MOTOR_SPEED",
					"variable_rotation":"NXT_MOTOR_ROTATION"
				}
			]	
		},
		"sequencers/copy.lua":	{
			"enable":				false,
			"name":					"COPY",
			"type":					"lua",					// required
			"add":					"COPY_ADD",				// required
			"remove":				"COPY_REMOVE",			// required
			"list":	[										// required
				{
					"variable_in_name":		"NXT_MOTOR_ROTATION",
					"variable_in_type":		"int",
					"variable_out_name":	"TEMPERATURE",
					"variable_out_type":	"float",
					"type":					"sampling"
				}
			]	
