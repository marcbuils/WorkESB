			"enable":				false,
			"name":					"HOT_COLD",
			"type":					"lua",					// required
			"variable_hotcold":		"HOT_COLD",				// required
			"variable_temperature":	"TEMPERATURE",			// required
			"event":				"HOT_COLD_CHANGE"
		},
		"sequencers/temperature.lua":	{
			"enable":				true,
			"name":					"SIMU TEMPERATURE",
			"type":					"lua",					// required
			"variable_temperature":	"TEMPERATURE",			// required
			"variable_time":		"TIME"					// required
		},
		"sequencers/time.lua":	{
			"enable":				false,
			"name":					"TIME",
			"type":					"lua",					// required
			"variable_time":		"TIME"					// required
